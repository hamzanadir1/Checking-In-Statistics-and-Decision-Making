          SQLTask:SqlStatementSource="BEGIN TRY&#xA;&#xA;DECLARE @startDate  DATETIME &#xA;DECLARE @endDate  DATETIME&#xA;&#xA;SET     @startDate = '2010-01-01 00:00:00'&#xA;SET     @endDate = CONVERT(VARCHAR,'12/31/2013',103)&#xA;&#xA;;&#xA;&#xA;WITH Temps AS&#xA;( &#xA; SELECT  @startDate AS Instant&#xA; UNION ALL&#xA;   SELECT       DATEADD(DAY, 1, Instant) AS Instant_1&#xA;   FROM         Temps AS Temps_1&#xA;   WHERE        (Instant &lt; @endDate )&#xA;)&#xA;&#xA;MERGE  DIM_Temps AS DIM_TMP&#xA;USING   (&#xA;       SELECT  Instant &#xA; , DATEPART(YEAR, Instant)AS Annee&#xA; , DATEPART(MONTH, Instant)AS Mois&#xA;        , DATEPART(DAY, Instant)AS Jour&#xA; , DATENAME(MONTH, Instant) AS NomMois&#xA; , DATENAME(WEEKDAY, Instant) AS NomJour&#xA;        , 0 AS EstFerie&#xA;        , ((DATEPART(WEEKDAY, Instant)% 6) * (DATEPART(WEEKDAY, Instant)% 7))AS EstOuvrable&#xA;       FROM  Temps&#xA;     ) AS TMP&#xA;    &#xA;ON     DIM_TMP.PK_Temps = TMP.Instant&#xA;&#xA;WHEN NOT MATCHED THEN&#xA;INSERT (PK_Temps, Annee, Mois, Jour, NomMois, NomJour, EstFerie, EstOuvrable)&#xA; VALUES &#xA;  ( &#xA; TMP.Instant &#xA;     , TMP.Annee&#xA;     , TMP.Mois&#xA;     , TMP.Jour&#xA;     , TMP.NomMois&#xA; , TMP.NomJour &#xA;     , TMP.EstFerie&#xA;     , TMP.EstOuvrable&#xA;  )&#xA;&#xA;&#xA;OPTION (MAXRECURSION 0)&#xA;;&#xA;END TRY&#xA;&#xA;BEGIN CATCH&#xA;PRINT 'Le message d''erreur est: ' ;&#xA;PRINT ERROR_MESSAGE();&#xA;&#xA;PRINT 'Le numéro de la ligne d''erreur est: ' ;&#xA;PRINT ERROR_LINE();&#xA;&#xA;PRINT 'Le code de l''erreur est: ' ;&#xA;PRINT ERROR_NUMBER();&#xA;&#xA;PRINT 'Le rang de la section d''erreur est: ' ;&#xA;PRINT ERROR_STATE();&#xA;END CATCH" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
